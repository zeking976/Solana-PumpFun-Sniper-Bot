# Solana wallet private key (base58 encoded)
PRIVATE_KEY=your_private_key_here

# Solana RPC endpoints
RPC_ENDPOINT=https://api.mainnet-beta.solana.com
RPC_WEBSOCKET_ENDPOINT=wss://api.mainnet-beta.solana.com

# Telegram bot token for sending reports
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Number of buys per 30-day cycle (default is 10)
NUM_BUYS=10

# Buy amount in SOL per transaction
BUY_AMOUNT=0.1

# Trading fees (in SOL, adjust based on network conditions)
TRANSACTION_FEE=0.001

# Filter thresholds
MIN_LIQUIDITY_USD=1000
MAX_TOKEN_AGE_MINUTES=60
MAX_TOP_10_HOLDERS_PERCENT=50


#ENV filename is always t.env
